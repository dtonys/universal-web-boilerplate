  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all clean dev-server",
    "dev-server": "cross-env NODE_ENV=development babel-watch src/server/server.js",
    "build": "npm-run-all clean build-client build-server build-node",
    "build-client": "cross-env NODE_ENV=production webpack --env production --progress -p --config webpack/webpack.client.config.js",
    "build-server": "cross-env NODE_ENV=production webpack --env production --progress -p --config webpack/webpack.server.config.js",
    "build-node": "cross-env NODE_ENV=production babel src/server -d build/server",
    "start": "cross-env NODE_ENV=production node build/server/server.js",
    "clean": "rimraf build/client build/server",
    "test": "jest --config jest.config.js",
    "lint-js": "eslint -c .eslintrc.js ./src"
  },
